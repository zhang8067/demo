<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zz.mapper.CategoryMapper">
<select id="queryAllCategorys" resultType="com.zz.vo.CategoryVO">
    select id,name,icon
    from tb_category
    ORDER BY create_time desc
</select>
</mapper>